<configuration>
    <appender name="jsonConsoleAppender" class="ch.qos.logback.core.FileAppender">
        <file>/logs/console.log</file>
        <encoder class="net.logstash.logback.encoder.LogstashEncoder"/>
    </appender>
    <root level="DEBUG">
        <appender-ref ref="jsonConsoleAppender"/>
    </root>
</configuration>